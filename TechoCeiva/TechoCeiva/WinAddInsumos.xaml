<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="TechoCeiva.WinAddInsumos"
	x:Name="winAddInsumo"
	Title="Agregar Insumo"
	Width="379" Height="179" FontSize="14.667">
	<Grid x:Name="LayoutRoot" Margin="0,0,-16,-39">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="0.686*"/>
			<ColumnDefinition Width="0.314*"/>
		</Grid.ColumnDefinitions>
		<Label Content="Insumo:" HorizontalAlignment="Left" Margin="26,8,0,0" VerticalAlignment="Top" FontSize="14.667"/>
		<TextBox Height="24" Margin="26,37.507,-11,0" Style="{DynamicResource SimpleTextBox}" TextWrapping="Wrap" Text="&#xa;" VerticalAlignment="Top" FontSize="14.667"/>
		<Button Content="Agregar" Margin="26,85,94,58" Style="{DynamicResource SimpleButton}" Background="#FF0092DD" Foreground="White" FontSize="14.667"/>
		<Button Content="Cerrar" Margin="-49,85,28,58" Style="{DynamicResource SimpleButton}" Foreground="White" Background="#FFC52023" FontSize="14.667" Grid.Column="1"/>
	</Grid>
</Window>